<div{{ attributes }}>
  {{ title_prefix }}
  {% if label %}
    <h2{{ title_attributes.addClass('text-2xl') }}>{{ label }}</h2>
  {% endif %}
  {{ title_suffix }}
  <div class="bg-blue text-align-justify">This is using tailwindcss</div>
  {% block content %}
    {{ content }}
  {% endblock %}
</div>
